{
    "RoslynExtensionsOptions": {
        "EnableAnalyzersSupport": true,
        "LocationPaths": [
            "./NuGet"
        ]
    },
    "fileOptions": {
        "userExcludeSearchPatterns": ["**/scripts/**/*"]
    },
    "excludeSearchPatterns": ["**/scripts/**/*"],
    "files.exclude": {
        "**/scripts/**/*": true
    },
    "files.watcherExclude": {
        "**/scripts/**/*": true
    },
    "search.exclude": {
        "**/scripts/**/*": true
    }
}